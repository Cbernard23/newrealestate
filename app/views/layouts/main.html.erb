    <%= yield %>
    <div class="row mt-5">
      <div class="form-wrapper">
        <div class="alert alert-info text-center form-custom">
          <h1>Contact Haaby Real Estate</h1>
          <%= form_tag '/mail' do %>
          <div class="form-row form-group">
            <div class="form-name">
              <%= label_tag :name, "First and Last Name" %>
              <%= text_field_tag :name, "", class: "form-control", placeholder: "First and Last Name" %>
            </div>
          </div>
          <div class="form-row form-group">
            <div class="form-email">
              <%= label_tag :email, "Email" %>
              <%= email_field_tag :email, "", class: "form-control", placeholder: "email@example.com" %>
              </div>
          </div>
          <div class="form-row form-group">
            <div class="form-phone">
              <%= label_tag :phone, "Phone Number" %>
              <%= telephone_field_tag :phone, "", class: "form-control", placeholder: "xxx-xxx-xxxx" %>
            </div>
          </div>
          <div class="form-row form-group">
            <div class="form-checks row pl-3">
              <div>
                <%= check_box_tag :buying %>
                <%= label_tag :buying, "Buying", class: "pr-3" %>
              </div>
              <div>
                <%= check_box_tag :selling %>
                <%= label_tag :selling, "Selling", class: "pr-3" %>
              </div>
              <div>
                <%= check_box_tag :mortgage %>
                <%= label_tag :mortgage, "Mortgage", class: "pr-3" %>
              </div>
              <div>
                <%= check_box_tag :newconstruction %>
                <%= label_tag :newconstruction, "New Construction", class: "pr-3" %>
              </div>
              <div>
                <%= check_box_tag :insurance %>
                <%= label_tag :insurance, "Insurance", class: "pr-3" %>
              </div>
              <div>
                <%= check_box_tag :other %>
                <%= label_tag :other, "Other" %>
              </div>
            </div>
          </div>
          <div class="form-row form-group">
            <div class="form-comments">
              <%= label_tag :comments, "Comments" %>
              <%= text_area_tag :comments, "", class: "form-control" %>
            </div>
          </div>
          <div class="form-row form-group">
            <div class="form-submit">
              <%= submit_tag "SUBMIT", class: "form-control" %>
            </div>
          </div>
          <% end %>
        </div>
      </div>
    </div>
